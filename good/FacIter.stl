list<int> range(int n) {
    if (n <= 1) {
        return int[];
    } else {
        n--;
        return n $ range(n);
    }
}

int factorial(int n) {
    int result = 1;
    for i in range(n + 1) {
        result *= i;
    }
    return result;
}

void main() {
    assert(factorial(10) == 3628800);
}
